
#!/bin/sh

# this is just an example!
# add your autostart stuffs here
picom &
#selekey & # its a script because a bug, selekey = setxkbmap us -variant altgr-intl 
xrdb merge ~/.Xresources &
feh --bg-fill ~/.config/slnxwm/bg/wallpaper.png &
################## Statusbar loop
while true; do
   xsetroot -name "$( date +"%I:%M %p" ) "
   sleep 1m    # Update time every minute
done &
################################3
# run bar script and dwm ( do not remove this )
dwm
